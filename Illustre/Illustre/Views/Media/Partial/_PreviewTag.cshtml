@model PreviewTagModel

<div class="card">
    <img src="@Model.Image" class="card-img-top">
    <div class="card-body">
        <a role="button"
           class="btn btn-dark"
           asp-controller="Media"
           asp-action="Search"
           asp-route-tagId="@Model.TagId"
           asp-route-isLiked="true">@Model.Title</a>
    </div>
</div>
